# Gemini API Key (required for product identification and QC analysis)
GEMINI_API_KEY=your_gemini_api_key_here

# Cloudflare Worker URL for image proxy (required for fetching images from product URLs)
# This should be the URL where your Cloudflare Worker is deployed
# Example: https://your-worker.your-subdomain.workers.dev
VITE_IMAGE_PROXY_URL=https://your-cloudflare-worker-url.workers.dev
